/*------------------------------------------------------------------------//
	05b. .10 Tab Navigation
//------------------------------------------------------------------------*/

.ui-tabs-vertical {     

	right: -500px;
	top: 0;
	width: 70%;
	z-index:52;
}

.ui-tabs-vertical .ui-tabs-nav { 
    clear: both;
    float: left;
    left: 0;
    margin-right: 1px;
    position: relative;
    width: 10%;
    z-index: 50;
}

.ui-tabs-vertical .ui-tabs-nav li { 
    background-color: #FFFFFF;
    border-bottom: 1px solid #CCCCCC;
    border-left: 1px solid #CCCCCC;
    border-radius: 5px 0 0 5px;
    border-right-width: 0 !important;
    border-top: 1px solid #CCCCCC;
    clear: left;
    margin: 8px -1px 0 8px;
    padding: 15px 0 15px 10px;
}

.ui-tabs-vertical .ui-tabs-nav li a { 
	display:block; 
	text-decoration:none;
	font-size:14px;
	color:#0E76BC;
}

.ui-tabs-vertical .ui-tabs-nav li.ui-tabs-active { 
    border-right-width: 1px;
    margin-bottom: 8px;
    padding-bottom: 15px;
    padding-top:15px;
    padding-right: 0.1em;
    width: 78%;
    box-shadow: -8px 0 12px #CCCCCC;
 	-moz-box-shadow: -8 0px 12px #ccc;
	-webkit-box-shadow: -8px 0px 12px #ccc;   
}

.ui-tabs-vertical .ui-tabs-panel { 
    background-color: #FFFFFF;
    border: 1px solid #CCCCCC;
    float: left;
    height: 360px;
    overflow: hidden;
    padding: 1em;
    width: 60%;
}

.ui-tabs-vertical .ui-tabs-panel h2 {
	color:#0E76BC;
	font-size:20px;
	margin-bottom:8px;
}
.ui-tabs-vertical .ui-tabs-panel p{
	font-size:12px;
	color:#999;
}

.slide {
  position: relative;
  top:500px;
  width:80%;
}

.slide .inner {
  position: absolute;
  left: 0;
}

.tab-wrapper {
  position: relative;
  width:1000px;
  /*right: -1080px;
  top: -1200px; */
  display:none; /* hide for now while working on sub pages */
}

.ui-tabs-vertical .ui-tabs-panel > h2 {
  float: left;
  font-size: 22px;
  margin-right: 20px;
}

.ui-tabs-vertical .ui-tabs-panel > h5 {
	color:#0E76BC;
	float: left;
	font-size: 15px;
	text-transform: uppercase;
	padding-top:7px;
}
.ui-tabs-vertical .ui-tabs-panel > select {
	border: 1px solid #CCCCCC;
	border-radius: 5px 5px 5px 5px;
	float: right;
	padding: 3px;
}

.hold {
   margin: 10px;
   position: relative;
   background: #eee;       
   height: 30px;
}
#innerscroll {
   position: absolute;
   background: #faa;
   left: 50%;
   height: 30px;
   width: 30px;
}

