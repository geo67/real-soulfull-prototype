	color:#000;
	text-decoration:none;
}

.page-wrapper footer > ul > li > a:hover{
	color:#ccc;
	text-decoration:none;
}

.page-wrapper .inner-footer {
    display: block;
    margin: 10px auto 0;
    padding: 10px 0 0;
    position: static;
}

